# 指明依赖库的来源地址
source 'https://github.com/CocoaPods/Specs.git'

# 写在最上面,表示公用
platform :ios, '9.0'

# 全局禁止显示警告
inhibit_all_warnings!

# 指定项目
target :'NYHEmptyData' do

   pod 'MJRefresh', '~> 3.2.0'
   pod 'MJExtension', '~> 3.0.17'
   pod 'Masonry', '~> 1.1.0'
   pod 'AFNetworking', '~> 3.2.1'
   pod 'SDWebImage', '~> 5.0.6'
   pod 'IQKeyboardManager', '~> 6.4.0'
   pod 'Toast', '~> 4.0.0'
   
end

