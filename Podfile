# 指明依赖库的来源地址
source 'https://github.com/CocoaPods/Specs.git'

# 写在最上面,表示公用
platform :ios, '9.0'

# 全局禁止显示警告
inhibit_all_warnings!

# 指定项目
target :'NYHEmptyData' do

   pod 'MJRefresh'
   pod 'MJExtension'
   pod 'Masonry'
   pod 'AFNetworking'
   pod 'SDWebImage'
   pod 'IQKeyboardManager'
   pod 'Toast'
   
end

